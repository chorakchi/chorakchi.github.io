{
  "name": "react-webpack-skeleton",
  "version": "1.0.2",
  "description": "React Webpack Skeleton",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "dev": "node dev-server.js",
    "lint": "node_modules/.bin/eslint --ext .js .",
    "test": "NODE_ENV=test NODE_PATH=. ./node_modules/.bin/mocha test/setup.js test --recursive",
    "webdriver:update": "./node_modules/.bin/webdriver-manager update",
    "webdriver:start": "./node_modules/.bin/webdriver-manager start",
    "pree2e": "npm run webdriver:update -- --standalone"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/knledg/react-webpack-skeleton.git"
  },
  "author": "Consolidated Knowledge",
  "license": "MIT",
  "homepage": "https://github.com/knledg/react-webpack-skeleton#readme",
  "dependencies": {
    "axios": "^0.9.1",
    "classnames": "^2.2.5",
    "extract-text-webpack-plugin": "^1.0.1",
    "lodash": "^4.7.0",
    "moment": "^2.12.0",
    "react": "^0.14.8",
    "react-autosuggest": "^3.7.4",
    "react-blur-admin": "^0.10.0",
    "react-dom": "^0.14.8",
    "react-flex-proto": "^1.0.0",
    "react-gl-maps": "git+https://github.com/knledg/react-gl-maps.git",
    "react-router": "^2.8.1",
    "react-tap-event-plugin": "^0.2.2"
  },
  "devDependencies": {
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.6.5"
  }
}
